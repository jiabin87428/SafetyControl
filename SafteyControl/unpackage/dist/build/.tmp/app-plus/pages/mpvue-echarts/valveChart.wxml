<view class="baseView"><view class="title">{{title}}</view><view class="canvasView"><mpvue-echarts class="vue-ref" vue-id="1" echarts="{{echarts}}" onInit="{{lineInit}}" canvasId="line" data-ref="lineChart" bind:__l="__l"></mpvue-echarts></view><view class="cellInfoView"><uni-list vue-id="2" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{xLine}}" wx:for-item="name" wx:for-index="index" wx:key="index"><uni-list-item vue-id="{{('3-'+index)+','+('2')}}" title="{{name}}" subnote="{{lineData[index]}}" show-arrow="false" bind:__l="__l"></uni-list-item></block></uni-list></view><hj-dragabledrawer vue-id="4" options="{{options}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="fliterView"><text class="fliterText">检查内容</text><view class="pickerView"><picker value="{{lx}}" range="{{checkDatas}}" data-event-opts="{{[['change',[['checkChange',['$event']]]]]}}" bindchange="__e"><view class="pickerItem">{{lx==''?'请选择检查内容':lx}}</view></picker></view><text class="fliterText">楼层</text><view class="pickerView"><picker value="{{lc}}" range="{{floorDatas}}" data-event-opts="{{[['change',[['floorChange',['$event']]]]]}}" bindchange="__e"><view class="pickerItem">{{lc==''?'请选择楼层':lc}}</view></picker></view><text class="fliterText">阀组编号</text><view class="pickerView"><picker value="{{fz}}" range="{{valveNumDatas}}" data-event-opts="{{[['change',[['valveChange',['$event']]]]]}}" bindchange="__e"><view class="pickerItem">{{fz==''?'请选择阀组编号':fz}}</view></picker></view><text class="fliterText">开始日期</text><view class="pickerView"><picker mode="date" value="{{beginDate}}" data-event-opts="{{[['change',[['beginDateChange',['$event']]]]]}}" bindchange="__e"><view class="pickerItem">{{beginDate==''?'请选择开始日期':beginDate}}</view></picker></view><text class="fliterText">结束日期</text><view class="pickerView"><picker mode="date" value="{{endDate}}" data-event-opts="{{[['change',[['endDateChange',['$event']]]]]}}" bindchange="__e"><view class="pickerItem">{{endDate==''?'请选择结束日期':endDate}}</view></picker></view><view data-event-opts="{{[['tap',[['searchClick',['$event']]]]]}}" class="btnView" bindtap="__e">确定</view></view></hj-dragabledrawer></view>